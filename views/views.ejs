<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Customers</title>
  </head>
  <body>
    <h1>Customer List</h1>
    <ul>
      <% customers.forEach(customer => { %>
      <li><%= customer.name %> - <%= customer.email %></li>
      <% }) %>
    </ul>
  </body>
</html>
